{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "dist-electron",
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es2017",
        "module": "commonjs",
        "jsx": "react",
        "typeRoots": [
            "node_modules/@types"
        ],
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ],
        "lib": [
            "es2017",
            "dom"
        ]
    }
}
